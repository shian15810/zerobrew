<header class="site-header">
  <nav class="site-nav" aria-label="Primary">
    <a class="site-brand" href="/">{{ site.title }}</a>
    {% if site.links and site.links.length %}
      <ul class="site-links">
        {% for link in site.links %}
          <li><a href="{{ link.href }}">{{ link.label }}</a></li>
        {% endfor %}
      </ul>
    {% endif %}
  </nav>
</header>
